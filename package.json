{
    "name": "frontend-path",
    "version": "1.0.0",
    "scripts": {
        "build": "tsc && rimraf dist && webpack",
        "debug": "dotenv -- nodemon -e ts,graphql,gql -x ts-node --inspect src/index.ts",
        "start": "nodemon -e ts,graphql,gql -x ts-node -r tsconfig-paths/register src/index.ts",
        "eslint": "eslint . --ext .ts",
        "prettier": "prettier --config .prettierrc --write \"{.,src/**}/*.{js,jsx,ts,tsx}\"",
        "test": "cross-env NODE_ENV=test jest",
        "tests": "cross-env NODE_ENV=test jest"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-typescript": "^7.9.0",
        "@types/glob": "^7.1.1",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.11.1",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "babel-jest": "^25.3.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "dotenv-cli": "^3.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^25.3.0",
        "nodemon": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.4",
        "rimraf": "^3.0.2",
        "start-server-webpack-plugin": "^2.2.5",
        "ts-loader": "^7.0.1",
        "ts-node": "^8.8.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.8.3",
